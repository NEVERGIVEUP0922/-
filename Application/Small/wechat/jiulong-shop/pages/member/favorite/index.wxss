.fui-list-inner .subtitle {
  font-size: 28rpx;
  color: #444;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  line-height: 32rpx;
}
.fui-list-media .round{
  width:200rpx;
  height:200rpx;
}
.fui-list-inner .row .subtitle{
  font-weight:600;
  font-size:32rpx;
}
.index_detail .text{
  height:70rpx;
  overflow:hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  display:-webkit-box;
}